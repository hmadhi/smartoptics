<h1>Consult History <%=link_to 'NewConsult', new_patient_consult_path(@patient.id) %></h1> 
 <table>
  <tr>
     <th>Service Date</th>
    <th>Reservation#</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>
</table>
<% patient_consults.each do |consult| %>
<table>
  <tr>
        <td><%= consult.service_date %></td>
        <td><%= consult.reservation_number %></td>
    	<td><%= link_to 'Edit', edit_patient_consult_path(consult.patient_id,consult.id) %></td>
    	<td><%= link_to 'Delete', patient_consult_path(consult.patient_id,consult.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
  </table>
<%end%>
